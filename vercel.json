{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VOLCANO_API_KEY": "${VOLCANO_API_KEY}",
    "VOLCANO_API_URL": "${VOLCANO_API_URL}",
    "VITE_BAIDU_APP_ID": "${VITE_BAIDU_APP_ID}",
    "VITE_BAIDU_SECRET_KEY": "${VITE_BAIDU_SECRET_KEY}",
    "BAIDU_APP_ID": "${VITE_BAIDU_APP_ID}",
    "BAIDU_SECRET_KEY": "${VITE_BAIDU_SECRET_KEY}",
    "KV_URL": "${KV_URL}",
    "KV_REST_API_URL": "${KV_REST_API_URL}",
    "KV_REST_API_TOKEN": "${KV_REST_API_TOKEN}",
    "KV_REST_API_READ_ONLY_TOKEN": "${KV_REST_API_READ_ONLY_TOKEN}"
  }
}